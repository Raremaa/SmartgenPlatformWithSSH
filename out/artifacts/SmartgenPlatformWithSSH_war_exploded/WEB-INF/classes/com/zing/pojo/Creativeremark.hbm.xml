<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zing.pojo">

    <class name="Creativeremark" table="creativeremark">
        <id name="id" column="Creremark_id"/>
        <property name="userId" column="User_id"/>
        <property name="creprojectId" column="Creproject_id"/>
        <property name="creremarkContent" column="Creremark_content"/>
        <property name="creremarkPraise" column="Creremark_praise"/>
        <property name="creremarkCommentTime" column="Creremark_commentTime"/>
    </class>
</hibernate-mapping>