<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zing.pojo">

    <class name="Purchase" table="purchase">
        <id name="id" column="Id"/>
        <property name="purchaseNo" column="Purchase_no"/>
        <property name="purchasePaymentTime" column="Purchase_paymentTime"/>
        <property name="purchasePatternOfPayment" column="Purchase_patternOfPayment"/>
        <property name="purchaseState" column="Purchase_state"/>
        <property name="purchaseCount" column="Purchase_count"/>
        <property name="purchaseClassify" column="Purchase_classify"/>
        <property name="purchaseName" column="Purchase_name"/>
        <property name="purchasePrice" column="Purchase_price"/>
        <property name="purchaseMsg" column="Purchase_msg"/>
        <property name="purchaseFreight" column="Purchase_freight"/>
        <property name="purchaseTime" column="Purchase_time"/>
        <property name="purchaseWay" column="Purchase_way"/>
        <many-to-one name="user" column="User_id" class="User"/>
        <set name="products" table="product_purchase_relation">
            <key column="Purchase_id"/>
            <many-to-many class="Product" column="Product_id"/>
        </set>
        <many-to-one name="productevalute" column="productevalute_id" class="Productevalute" unique="true"/>
    </class>
</hibernate-mapping>